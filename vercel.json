{
  "version": 2.0,
  "builds": [
    {
      "src": "src/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/style1.css",
      "use": "@vercel/static"
    },
    {
      "src": "src/style2.css",
      "use": "@vercel/static"
    },
    {
      "src": "src/script.js",
      "use": "@vercel/static"
    },
    {
      "src": "image/*",
      "use": "@vercel/static"
    },
    {
      "src": "sound/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/$1"
    }
  ]
}
